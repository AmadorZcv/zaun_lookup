  <li>
    <strong>Game type:</strong>
    <%= @match.game_type %>
  </li>

  <li>
    <strong>Game duration:</strong>
    <%= @match.game_duration %>
  </li>

  <li>
    <strong>Game creation:</strong>
    <%= @match.game_creation %>
  </li>

  <li>
    <strong>Winning team:</strong>
    <%= @match.winning_team %>
