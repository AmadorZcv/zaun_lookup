<h1>Show Match</h1>

<ul>

  <li>
    <strong>Season:</strong>
    <%= @match.season_id %>
  </li>

  <li>
    <strong>Queue:</strong>
    <%= @match.queue_id %>
  </li>

  <li>
    <strong>Game:</strong>
    <%= @match.game_id %>
  </li>

  <li>
    <strong>Game version:</strong>
    <%= @match.game_version %>
